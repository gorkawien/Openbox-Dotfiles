		<menu id="system" label="Sistema">
			<item label="Gestor de archivos (root)">
				<action name="Execute">
					<execute>
						pkexec pcmanfm /
					</execute>
				</action>
			</item>
			<item label="Editar repositorios">
				<action name="Execute">
					<execute>
						pkexec mousepad /etc/apt/sources.list
					</execute>
				</action>
			</item>
            <item label="Logs del Sistema">
				<action name="Execute">
					<execute>
						pkexec /usr/bin/gnome-system-log
					</execute>
				</action>
			</item>
			<item label="Información del Sistema">
				<action name="Execute">
					<execute>
				       xterm -fa 'Monospace' -fs 12 -hold -e inxi -F
					</execute>
				</action>
			</item>
			<item label="Lxtask">
				<action name="Execute">
					<execute>/usr/bin/lxtask</execute>
