			<menu id="graphicsScreenshots" label="Captura">
				<item label="Captura de Pantalla">
					<action name="Execute">
						<execute>
							xfce4-screenshooter
						</execute>
					</action>
				</item>
				<separator label="Cuando"/>
				<item label="Ahora">
					<action name="Execute">
						<execute>
							scrot '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; mirage ~/$f'
						</execute>
					</action>
				</item>
				<item label="En 5 segundos...">
					<action name="Execute">
						<execute>
							scrot -d 5 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; mirage ~/$f'
						</execute>
					</action>
				</item>
				<item label="En 10 segundos...">
					<action name="Execute">
						<execute>
							scrot -d 10 '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; mirage ~/$f'
						</execute>
					</action>
				</item>
				<item label="Selecionar Superficie... (користи миш)">
					<action name="Execute">
						<execute>
							scrot -s '%Y-%m-%d--%s_$wx$h_scrot.png' -e 'mv $f ~/ &amp; mirage ~/$f'
						</execute>
					</action>
				</item>
			</menu>
